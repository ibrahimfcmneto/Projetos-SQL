DROP TABLE ABASTECIMENTOS;
DROP TABLE ENTREGAS;
DROP TABLE TANQUES;
DROP TABLE PAGAMENTOS;
DROP TABLE COMPRAS;
DROP TABLE CONTATOS;
DROP TABLE FORNECEDORES;
DROP TABLE COMBUSTIVEIS;
DROP TABLE MODALIDADE_PGTO;  

CREATE TABLE COMBUSTIVEIS(
    CODIGO NUMBER GENERATED ALWAYS AS IDENTITY PRIMARY KEY, 
    NOME VARCHAR2(50) NOT NULL,
    SIGLA VARCHAR2(4) NOT NULL
);


CREATE TABLE TANQUES(
    CODIGO NUMBER GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    NOME VARCHAR2(100),
    CAPACIDADE NUMBER(7) NOT NULL, 
    VOLUME_OCUPADO NUMBER(7) NOT NULL,
    VOLUME_DISPONIVEL NUMBER(7) NOT NULL
);


CREATE TABLE MODALIDADE_PGTO(
    CODIGO NUMBER GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    DESCRICAO VARCHAR2(500)
);


CREATE TABLE FORNECEDORES(
    CODIGO NUMBER GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    RAZAO_SOCIAL VARCHAR2(500) NOT NULL,
    CNPJ VARCHAR2(20) NOT NULL UNIQUE,
    IE VARCHAR(20) NOT NULL UNIQUE,
    TELEFONE VARCHAR2(20)
);


CREATE TABLE CONTATOS (
    CODIGO NUMBER GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    NOME VARCHAR2(500) NOT NULL,
    TELEFONE_FIXO VARCHAR2(20),
    TELEFONE_CELULAR VARCHAR2(20),
    WHATSAPP VARCHAR2(20),
    DATA_INICIO DATE DEFAULT SYSDATE,
    DATA_FIM DATE,

    FORNECEDOR NUMBER NOT NULL,

    CONSTRAINT FK_CONTATO_FORNECEDOR FOREIGN KEY (FORNECEDOR) 
    REFERENCES FORNECEDORES (CODIGO) 
);


CREATE TABLE COMPRAS(
    CODIGO NUMBER GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    LITROS NUMBER(9) NOT NULL,
    VALOR_TOTAL_BRUTO NUMBER(10,2) NOT NULL,
    VALOR_TOTAL_LIQUIDO NUMBER(10,2) NOT NULL,
    DATA_COMPRA NUMBER(10,2) NOT NULL,

    COMBUSTIVEL NUMBER NOT NULL,
    FORNECEDOR NUMBER NOT NULL,
    CONTATO NUMBER NOT NULL,
    MODALIDADE_PGTO NUMBER NOT NULL,

    CONSTRAINT FK_COMPRA_COMBUSTIVEL FOREIGN KEY (COMBUSTIVEL)
    REFERENCES COMBUSTIVEIS(CODIGO),

    CONSTRAINT FK_COMPRA_FORNECEDOR FOREIGN KEY (FORNECEDOR)
    REFERENCES FORNECEDORES(CODIGO),
    
    CONSTRAINT FK_COMPRA_CONTATO FOREIGN KEY (CONTATO)
    REFERENCES CONTATOS(CODIGO),
    
    CONSTRAINT FK_COMPRA_MODALIDADE_PGTO FOREIGN KEY (MODALIDADE_PGTO)
    REFERENCES MODALIDADE_PGTO(CODIGO)
    
);


CREATE TABLE PAGAMENTOS(
    CODIGO NUMBER GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    VALOR_ESTIMADO NUMBER NOT NULL,
    VALOR_EFETIVO NUMBER NOT NULL,
    DATA_VENCIMENTO DATE NOT NULL,
    DATA_EFETIVA_PAGAMENTO DATE,
    COMPRA NUMBER NOT NULL,

    CONSTRAINT FK_PAGAMENTO_COMPRA FOREIGN KEY (COMPRA) REFERENCES COMPRAS(CODIGO)
);


CREATE TABLE ENTREGAS(
    CODIGO NUMBER GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    DATA_ESTIMADA DATE NOT NULL,
    DATA_EFETIVA DATE, 
    STATUS NUMBER(1) NOT NULL,
    COMPRA NUMBER NOT NULL,
    CONSTRAINT FK_ENTREGA_COMPRA FOREIGN KEY (COMPRA) REFERENCES 
    COMPRAS(CODIGO)
);


CREATE TABLE ABASTECIMENTOS(
    CODIGO NUMBER GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    LITROS NUMBER(9) NOT NULL,
    COMBUSTIVEL NUMBER NOT NULL, 
    TANQUE NUMBER NOT NULL,
    ENTREGA NUMBER NOT NULL, 

    CONSTRAINT FK_ABASTECIMENTO_ENTREGA FOREIGN KEY (ENTREGA)
    REFERENCES ENTREGAS(CODIGO),
    
    CONSTRAINT FK_ABASTECIMENTO_COMB FOREIGN KEY (COMBUSTIVEL)
    REFERENCES COMBUSTIVEIS(CODIGO),

    CONSTRAINT FK_ABASTECIMENTO_TANQUE FOREIGN KEY (TANQUE)
    REFERENCES TANQUES(CODIGO)
);


/*----------------------------------------------------------------------------------*/

